{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "baseUrl": "src",
    "paths": {
      "@cv":[ "cv.json" ],
      "@components/*": ["components/*"],
      "@static/*": ["static/*"],
      "@icons/*":[ "icons/*" ],
      "@/*":[ "src/*" ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": ["tailwind.config.js"]
}